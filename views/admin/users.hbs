<div class="container">
    <div class="row mt-4 mb-4 justify-content-center">
          <h2>Users</h2>
    </div>
    
    <table class="table mt-5" id="protable">
  <thead>
    <tr>
      
      <th scope="col">UserID</th>
      <th scope="col">Name</th>
      <th scope="col">Gmail</th>
     
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr>
     
      <td>{{this.UserID}}</td>
      <td>{{this.Name}}</td>
      <td>{{this.Email}}</td>

        
    </tr>
   {{/each}} 
  </tbody>
</table>
</div>

<script>
  $(function(){
    $('#protable').DataTable()
  })
</script>